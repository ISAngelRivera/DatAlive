{
  "dashboard": {
    "title": "DataLive GraphRAG Performance",
    "uid": "graphrag-performance",
    "panels": [
      {
        "title": "Query Distribution by Strategy",
        "targets": [
          {
            "expr": "sum(rate(datalive_queries_total[5m])) by (strategy)"
          }
        ],
        "type": "piechart"
      },
      {
        "title": "Average Query Latency by Strategy",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(datalive_query_duration_seconds_bucket[5m])) by (strategy)"
          }
        ],
        "type": "graph"
      },
      {
        "title": "Cache Hit Rate",
        "targets": [
          {
            "expr": "rate(datalive_cache_hits_total[5m]) / rate(datalive_cache_requests_total[5m])"
          }
        ],
        "type": "stat"
      },
      {
        "title": "Knowledge Graph Growth",
        "targets": [
          {
            "expr": "datalive_kg_nodes_total"
          },
          {
            "expr": "datalive_kg_relationships_total"
          }
        ],
        "type": "timeseries"
      },
      {
        "title": "Entity Extraction Performance",
        "targets": [
          {
            "expr": "rate(datalive_entities_extracted_total[5m])"
          }
        ],
        "type": "gauge"
      }
    ]
  }
}