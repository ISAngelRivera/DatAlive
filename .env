# DataLive Enterprise RAG+KAG System v3.0
# Variables de entorno - Copiar a .env y configurar

# =============================================================================
# CONFIGURACIÃ“N GENERAL
# =============================================================================
TZ=Europe/Madrid
NODE_ENV=production

# =============================================================================
# POSTGRESQL
# =============================================================================
POSTGRES_USER=datalive_user
POSTGRES_DB=datalive_db
POSTGRES_PASSWORD=adminpassword

# =============================================================================
# NEO4J KNOWLEDGE GRAPH
# =============================================================================
NEO4J_PASSWORD=adminpassword

# =============================================================================
# REDIS CACHE
# =============================================================================
REDIS_PASSWORD=change_this_redis_password

# =============================================================================
# MINIO OBJECT STORAGE
# =============================================================================
MINIO_ROOT_USER=datalive_admin
MINIO_ROOT_PASSWORD=change_this_minio_password

# =============================================================================
# N8N CONFIGURATION
# =============================================================================
N8N_LICENSE_KEY=
N8N_LICENSE_TENANT_ID=
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=adminpassword

# =============================================================================
# GRAFANA MONITORING
# =============================================================================
GRAFANA_USER=admin

# =============================================================================
# AGENT CONFIGURATION
# =============================================================================
LLM_MODEL=phi-4:latest
EMBEDDING_MODEL=nomic-embed-text:v1.5

# =============================================================================
# SECURITY & MONITORING
# =============================================================================
ENABLE_METRICS=true
LOG_LEVEL=INFO

# =============================================================================
# DEVELOPMENT FLAGS
# =============================================================================
DEBUG_MODE=false
ENABLE_SWAGGER=true